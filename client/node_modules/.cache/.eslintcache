  },\n}));\n\nconst Success = () => {\n  const { state } = useContext(AppContext);\n  const { buyer, orders } = state;\n  const { payment, products } = orders[orders.length - 1];\n  const location = useGoogleAddress(buyer.address);\n  const classes = useStyles();\n  return (\n    <Grid container spacing={4}>\n      <Grid item xs={12}>\n        <Typography variant=\"h4\">{`Dear ${\n          buyer.firstName || \"customer\"\n        }, thanks for your purchase`}</Typography>\n        <Typography variant=\"subtitle1\">\n          {`Your order ${payment.orderID} is arriving in 3 days to:`}\n        </Typography>\n      </Grid>\n      <Grid item xs={12} md={6}>\n        <GridList className={classes.root}>\n          {products &&\n            products.map((product) => (\n              <GridListTile key={product.title}>\n                <img\n                  alt={product.title}\n                  src={product.image}\n                  className={classes.img}\n                />\n                <GridListTileBar title={product.title} />\n              </GridListTile>\n            ))}\n        </GridList>\n      </Grid>\n      <Grid item xs={12} md={6}>\n        <Map location={location} />\n      </Grid>\n    </Grid>\n  );\n};\n\nexport default Success;\n","/home/rjmata95/Desktop/Learning/platzi/react/platzi-conf-merch/client/src/containers/NotFound.jsx",[],"/home/rjmata95/Desktop/Learning/platzi/react/platzi-conf-merch/client/src/initialState.js",["173"],"export default {\n  loading: false,\n  user: {\n    profile: {},\n    token: \"\",\n  },\n  cart: [],\n  buyer: {},\n  orders: [],\n  products: [\n    // {\n    //   id: \"1\",\n    //   image: \"https://arepa.s3.amazonaws.com/camiseta.png\",\n    //   title: \"Camiseta\",\n    //   price: 25,\n    //   description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit\",\n    // },\n    // {\n    //   id: \"3\",\n    //   image: \"https://arepa.s3.amazonaws.com/mug.png\",\n    //   title: \"Mug\",\n    //   price: 1